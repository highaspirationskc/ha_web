<div id="<%= dom_id event_registration %>" class="w-full sm:w-auto my-5 space-y-5">
  <div>
    <strong class="block font-medium mb-1">Event:</strong>
    <%= event_registration.event_id %>
  </div>
  <div>
    <strong class="block font-medium mb-1">User:</strong>
    <%= event_registration.user_id %>
  </div>
  <div>
    <strong class="block font-medium mb-1">Registration date:</strong>
    <%= event_registration.registration_date %>
  </div>
</div>
